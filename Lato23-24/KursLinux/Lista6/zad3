1.	- sudo systemctl status ssh (sudo apt-get install openssh-server)
2.	- ssh-keygen -t rsa -b 4096
3.	- ssh-copy-id -i dojana.pub jantest@localhost
	- ssh -i dojana jantest@localhost
4. 	- nano ~/.ssh/config
	Host localhost
	    HostName        localhost
	    User            jantest
	    IdentityFile    ~/University/Lato23-24/KursLinux/Lista6/dojana
5. 	- ssh-add -t 3600 dojana
	- ssh-add -D (eval $(ssh-agent))
6.	- sudo usermod -L jantest (su nie działa, ssh działa)
7.	- sudo visudo -> jantest ALL=(root:root) NOPASSWD: ALL
8. 	- sudo usermod -U jantest
	- sudo usermod --expiredate 1 jantest -> off
9.	- sudo usermod --expiredate "" jantest -> on
	- sudo usermod -s /bin/false jantest (logowanie nie działa, ale sudo -u jantest whoami tak)
	- dodatkowe info o tym że konto niedostępne
10.	- /etc/ssh/sshd_config -> PermitRootLogin no
11. - /etc/ssh/sshd_config -> PasswordAuthentication no
